{
	"name": "apiserver",
	"type": "python-service",
	"default_port": 8001,
	"envs": {
		"production": [
			"_SERVICE_HOST_PORT",
			"_USE_WSGI_PROTOCAL",
			"_SERVICE_MODE",
			"_REGISTER_SERVICE",
			"_DB_HOST",
			"_DB_PORT",
			"_REDIS_HOST",
			"_MONGO_HOST"
		],
		"dev": [
			"_IS_WEIZOOM_DEV_VM",
			"_SERVICE_HOST_PORT"
		]
	},
	"hosts": {
		"always": [
			"etcd.weizoom.com"
		],
		"dev": [
			"db.dev.com"
		],
		"production": [
		]
	},
	"dev": {
		"rebuild": "no",
		"init": "no"
	},

	"register": {
		"locations": [{
			"path": "/static",
			"root": ".",
			"type": "root"
		}],
		"proxy_locations": [{
			"service": "api-gateway",
			"path": "/default/",
			"type": "service_redirect"
		}]
	}
}